{
  "name": "nx-test-workspace",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tja4472/nx-test-workspace"
  },
  "license": "MIT",
  "scripts": {
    "=== emulators ===": "",
    "emulators:start-demo": "firebase emulators:start --project demo-1",
    "emulators:start-real": "firebase emulators:start",
    "=== my-angular-app-e2e ==": "",
    "my-angular-app-e2e:emulate-demo": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\" CYPRESS_USE_DEMO_PROJECT=\"true\" GCLOUD_PROJECT=\"demo-1\" ng e2e --watch -c emulator-demo",
    "my-angular-app-e2e:emulate-demo:running": "npm run my-angular-app-e2e:emulate-demo -- --baseUrl=http://localhost:4200 --devServerTarget=''",
    "my-angular-app-e2e:emulate-real": "cross-env FIRESTORE_EMULATOR_HOST=\"localhost:8080\" FIREBASE_AUTH_EMULATOR_HOST=\"localhost:9099\"  GCLOUD_PROJECT=\"emulators-codelab-a5a89\" ng e2e --watch -c emulator-real",
    "my-angular-app-e2e:emulate-real:running": "npm run my-angular-app-e2e:emulate-real -- --baseUrl=http://localhost:4200 --devServerTarget=''",
    "my-angular-app-e2e:real": "cross-env GOOGLE_APPLICATION_CREDENTIALS=\"../../serviceAccount-real.json\" ng e2e --watch -c development",
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test-all": "nx run-many --all --target=test",
    "test-all-emulator": "nx run-many --all --target=test --runInBand --group=emulator-required",
    "test-all-no-emulator": "nx run-many --all --target=test --group=-emulator-required",    
    "testA": "ng test --test-path-pattern=.*.emul.spec",
    "testAA": "ng test --test-path-pattern=emul.spec",
    "testB": "ng test --testMatch '**/+(*.)+(specA|test|emul).+(ts|js)?(x)'",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "e2e:my-angular-app": "nx e2e my-angular-app-e2e --watch",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "npm run firebase -- --version": "",
    "firebase": "firebase"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.4",
    "@angular/common": "12.2.4",
    "@angular/compiler": "12.2.4",
    "@angular/core": "12.2.4",
    "@angular/fire": "7.0.4",
    "@angular/forms": "12.2.4",
    "@angular/platform-browser": "12.2.4",
    "@angular/platform-browser-dynamic": "12.2.4",
    "@angular/router": "12.2.4",
    "@nrwl/angular": "12.9.0",
    "firebase": "9.0.2",
    "rxfire": "6.0.2",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.4",
    "@angular-eslint/eslint-plugin": "~12.3.0",
    "@angular-eslint/eslint-plugin-template": "~12.3.0",
    "@angular-eslint/template-parser": "~12.3.0",
    "@angular/cli": "12.2.4",
    "@angular/compiler-cli": "12.2.4",
    "@angular/language-service": "12.2.4",
    "@firebase/rules-unit-testing": "1.3.15",
    "@nrwl/cli": "12.9.0",
    "@nrwl/cypress": "12.9.0",
    "@nrwl/eslint-plugin-nx": "12.9.0",
    "@nrwl/jest": "12.9.0",
    "@nrwl/linter": "12.9.0",
    "@nrwl/tao": "12.9.0",
    "@nrwl/workspace": "12.9.0",
    "@types/jest": "26.0.24",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "cross-env": "7.0.3",
    "cypress": "^8.3.0",
    "cypress-firebase": "2.0.0",
    "dotenv": "10.0.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "firebase-admin": "9.11.1",
    "firebase-tools": "9.18.0",
    "jest": "27.0.3",
    "jest-preset-angular": "9.0.7",
    "jest-runner-groups": "2.1.0",
    "prettier": "^2.3.1",
    "ts-jest": "27.0.3",
    "typescript": "4.3.5"
  }
}
